PCI\VEN_18A2&DEV_0005&SUBSYS_000018A2.Desc   = "Standard H264 16 Ports Capture Card"
PCI\VEN_18A2&DEV_0006&SUBSYS_000018A2.Desc   = "Standard H264 16 Ports DVR Card"
PCI\VEN_18A2&DEV_0007&SUBSYS_000018A2.Desc   = "Stretch VRC6416-ba DVR Card"
