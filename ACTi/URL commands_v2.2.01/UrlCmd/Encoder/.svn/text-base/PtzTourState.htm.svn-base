	<a href="PtzPresetGet.htm">PTZ_PRESET_GET</a>, <a href="PtzPresetGo.htm">
	PTZ_PRESET_GO</a>, <a href="PtzTourSet.htm">
	PTZ_TOUR_SET</a>, <a href="PtzTourGet.htm">PTZ_TOUR_GET</a>,
